{
  "appName": "${JL_APP_NAME}",
  "rootDir": "${JL_APP_DATA_DIR}",
  "host": "${JL_APP_HOST}",
  "debug": false,
  "ts_node": true,
  "cluster": true,
  "port": 80,
  "apiPort": 4103,
  "socket_file": "${{rootDir}}/web.sock",
  "pid_file": "${{rootDir}}/tmp/server.pid",
  "postgres": {
    "url": "${JL_APP_POSTGRES}",
    "debug": true
  },
  "redis": {
    "url": "${JL_APP_REDIS}"
  },
  "hosts": {
    "main": {
      "addr": "t.jingli365.com",
      "port": 14201,
      "www": "https://${{hosts.main.addr}}"
    },
    "data": {

      "addr": "${JL_APP_DATA_STORE}",
      "port": "${JL_APP_DATA_STORE_PORT}"
    }
  },
  "agent": {
    "appId": "756b12b3-e243-41ae-982f-dbdfb7ea7e92",
    "appSecret": "6c8f2cfd-7aa4-48c7-9d5e-913896acec12"
  },
  "api": {
    "postgres": "${{postgres.url}}",
    "remotes": [
      {
        "connect": {
          "host": "${{hosts.main.addr}}",
          "port": "${{hosts.main.port}}"
        },
        "modules": [
          "place"
        ]
      },
      {
        "connect": {
          "host": "${{hosts.data.addr}}",
          "port": "${{hosts.data.port}}"
        },
        "modules": [
          "getData"
        ]
      }
    ]
  },
  "logger": {
    "path": "${{rootDir}}/log",
    "prefix": "${{appName}}_",
    "httptype": "short",
    "mods": {
      "sequelize": {
        "prefix": "${{appName}}_sql_",
        "console": false
      },
      "sqlinit": {
        "prefix": "${{appName}}_sql_",
        "console": false
      },
      "http": {
        "mods": {
          "access": {
            "console": false
          }
        }
      }
    }
  },
  "isUsingFakeData": false,
  "juHeCurrencyAPIKey": "58db4718504b1fa7a7448a8024c41864",
  "oldPostgres": {
    "url": "${JL_APP_OLD_POSTGRES_URL}"
  },
  "sessionTime": "20",
  "superUser": [
    "qmtrip@jingli365.com"
  ],
  "placeAPI": "https://l.jingli365.com/proj/svr-place"
}